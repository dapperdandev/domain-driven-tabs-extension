{
  "manifest_version": 3,
  "name": "Domain Driven Tabs",
  "description": "Automatically Organize Tabs by Domain",
  "version": "1.0.0",
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },
  "version_name": "1.0.0 beta",
  "permissions": ["tabs", "tabGroups", "scripting", "storage"],
  "action": {
    "default_popup": "index.html",
    "default_title": "Domain Driven Tabs"
  }
}
